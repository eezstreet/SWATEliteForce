///////////////////////////////////////////////////////////////////////////////

class DisableTargetGoal extends SwatCharacterGoal;

///////////////////////////////////////////////////////////////////////////////
//
// Variables

var(parameters) Actor  Target;

///////////////////////////////////////////////////////////////////////////////
//
// Constructor

overloaded function construct(AI_Resource r, Actor inTarget)
{
	super.construct( r );

	assert(inTarget != None);
	Target = inTarget;
}

///////////////////////////////////////////////////////////////////////////////

defaultproperties
{
	priority = 80
	goalName = "DisableTarget"
}

