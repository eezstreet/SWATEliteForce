///////////////////////////////////////////////////////////////////////////////
// MoveToGoal.uc - MoveToGoal class
// this goal is given to those Officers when they are given the move to command

class MoveToGoal extends OfficerCommandGoal;
///////////////////////////////////////////////////////////////////////////////

///////////////////////////////////////////////////////////////////////////////
//
// Variables

// copied to our action
var(parameters) Actor Destination;

///////////////////////////////////////////////////////////////////////////////
//
// Constructor

overloaded function Construct(AI_Resource r, Actor inDestination)
{
	super.construct(r, priority);

    Destination = inDestination;
}

///////////////////////////////////////////////////////////////////////////////
defaultproperties
{
    priority   = 75
    goalName   = "MoveTo"
}